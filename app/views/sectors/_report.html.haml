%tr
  %td= report.reportable.cell&.name
  %td= report.reportable.village&.name
  %td.text-center= report.plan&.goal
  %td.text-center= human_number report.distributed
  %td.text-center= human_number report.impact
  %td.text-center= human_number report.checked
  %td= link_to 'Edit', edit_report_path(report), class: 'btn yellow small', target: '_blank'
  %td= link_to 'Delete', report, method: :delete, class: 'btn red small village-report-delete', remote: true, data: { confirm: 'Are you sure?' }
